					},
					"AzureDataLakeCloudStg_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeCloudStg_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"FileServer_Local_properties_typeProperties_host": {
						"value": "[parameters('FileServer_Local_properties_typeProperties_host')]"
					},
					"FileServer_Local_properties_typeProperties_userId": {
						"value": "[parameters('FileServer_Local_properties_typeProperties_userId')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-First-Task-Dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
