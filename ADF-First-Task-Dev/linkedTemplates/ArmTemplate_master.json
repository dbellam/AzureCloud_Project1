					},
					"AzureDataLakeCloudStg_accountKey": {
						"value": "[parameters('AzureDataLakeCloudStg_accountKey')]"
					},
					"FileServer_Local_password": {
						"value": "[parameters('FileServer_Local_password')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
